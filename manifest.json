{
    "name": "Stash++",
    "version": "2.0.1",
    "manifest_version": 2,
    "description": "A Chrome extension to make stash better",
    "page_action": {
        "default_icon":"img/diff-icon.png",
        "default_title":"Stash Pull Request Diff"
    },
    "content_scripts": [{
        "js": ["js/lib/jquery-2.1.1.min.js",
            "js/app/content.js"],
        "css": ["css/content.css"],
        "matches": ["*://*/*"],
        "run_at": "document_start"
    }]
}
